# Hive Publisher environment configuration
# Copy this file to .env and configure values. Do NOT commit .env to source control.

# Hive account for write operations (optional for read-only)
HIVE_USERNAME=your_username
HIVE_POSTING_KEY=your_posting_private_key

# Network settings (optional): testnet or custom API node
HIVE_TESTNET=false    # true = testnet, false = mainnet
HIVE_API_NODE=https://api.hive.blog